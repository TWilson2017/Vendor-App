<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f7ffdd"
    tools:context=".Register_Vendor">

    <ImageView
        android:id="@+id/phone_icon"
        android:layout_width="246dp"
        android:layout_height="168dp"
        android:layout_gravity="center"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="15dp"
        android:contentDescription="TODO"
        android:src="@drawable/gotit_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046" />

    <EditText
        android:id="@+id/acct_num"
        android:layout_width="252dp"
        android:layout_height="28dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Account Number"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_num" />

    <EditText
        android:id="@+id/store_name"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="204dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:autofillHints=""
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Store Name"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.056" />

    <EditText
        android:id="@+id/street_address"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_width="253dp"
        android:layout_height="27dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Street Address"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/store_name"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/store_city"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_width="252dp"
        android:layout_height="26dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="City"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/street_address"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/zip_code"
        android:layout_width="109dp"
        android:layout_height="31dp"
        android:layout_marginBottom="90dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Zip "
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.314"
        app:layout_constraintStart_toEndOf="@+id/store_city"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.496" />

    <EditText
        android:id="@+id/tax_id"
        android:layout_width="236dp"
        android:layout_height="43dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:autofillHints=""
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Tax Id"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/store_city"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/state_id"
        android:layout_width="251dp"
        android:layout_height="25dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:autofillHints=""
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="State"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/store_city"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/phone_num"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"

        android:layout_width="253dp"
        android:layout_height="24dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Telephone"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tax_id"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/routing_num"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"

        android:layout_marginTop="36dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Routing Number"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/acct_num" />

    <EditText
        android:id="@+id/username"
        android:layout_width="252dp"
        android:layout_height="26dp"
        android:layout_marginEnd="144dp"
        android:layout_marginRight="144dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Username"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/acct_num" />

    <Button
        android:id="@+id/submit4_button"
        android:layout_width="145dp"
        android:layout_height="40dp"
        android:layout_marginTop="100dp"
        android:background="#B1CF5F"
        android:text="Submit"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.972" />

    <EditText
        android:id="@+id/password"
        android:layout_width="247dp"
        android:layout_height="40dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="148dp"
        android:layout_marginRight="148dp"
        android:ems="10"
        android:hint="password"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/routing_num" />

</androidx.constraintlayout.widget.ConstraintLayout>