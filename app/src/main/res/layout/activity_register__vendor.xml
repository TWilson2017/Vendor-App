<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register_Vendor"
    android:background="#f7ffdd">

    <ImageView
        android:id="@+id/phone_icon"
        android:layout_width="185dp"
        android:layout_height="135dp"
        android:layout_gravity="center"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="50dp"
        android:contentDescription="TODO"
        android:src="@drawable/gotit_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068" />

    <EditText
        android:id="@+id/acct_num"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Account Number"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_num" />

    <EditText
        android:id="@+id/store_name"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="148dp"
        android:layout_marginRight="148dp"
        android:autofillHints=""
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Store Name"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/street_address"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Street Address"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/store_name" />

    <EditText
        android:id="@+id/store_city"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="City"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/street_address" />

    <EditText
        android:id="@+id/zip_code"
        android:layout_width="90dp"
        android:layout_height="45dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="90dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Zip "
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toEndOf="@+id/store_city"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439" />

    <EditText
        android:id="@+id/tax_id"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="148dp"
        android:layout_marginRight="148dp"
        android:autofillHints=""
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Tax Id"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/store_city" />

    <EditText
        android:id="@+id/state_id"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginEnd="148dp"
        android:layout_marginRight="148dp"
        android:autofillHints=""
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="State"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/store_city" />

    <EditText
        android:id="@+id/phone_num"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Telephone"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tax_id" />

    <EditText
        android:id="@+id/routing_num"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:hint="Routing Number"
        android:inputType="textPersonName"
        android:textColor="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/acct_num"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/submit4_button"
        android:layout_width="145dp"
        android:layout_height="40dp"
        android:layout_marginTop="100dp"
        android:background="#B1CF5F"
        android:text="Submit"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.972" />

</androidx.constraintlayout.widget.ConstraintLayout>