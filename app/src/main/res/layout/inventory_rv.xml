        android:textColor="#2e4057"
        android:textSize="15sp" />

    <ImageButton
        android:id="@+id/deleteBtn"
        android:layout_width="48px"
        android:layout_height="39px"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
